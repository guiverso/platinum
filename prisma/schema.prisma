datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Compra {
  id                    String     @id @db.VarChar(16)
  data                  DateTime   @db.Date
  guia_trafego          String     @db.VarChar(32)
  autorizacao_aquisicao String     @db.VarChar(16)
  pdf                   Bytes?
  posicao               Int        @default(autoincrement())
  esgotada              Boolean    @default(false)
}n